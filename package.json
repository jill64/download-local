{
  "name": "@jill64/downloads-local",
  "version": "1.1.0",
  "type": "module",
  "description": "⬇️ Download local file via a-tag on browser",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/downloads-local/issues",
  "homepage": "https://github.com/jill64/downloads-local#readme",
  "files": [
    "dist",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/downloads-local.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "browser",
    "download",
    "local",
    "utility"
  ],
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier --write ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint-plugin-deprecation": "2.0.0",
    "typescript": "5.3.3"
  }
}
